    "@hyperledger/caliper-cli": "^0.6.0",
    "@hyperledger/caliper-core": "^0.6.0",
    "@hyperledger/caliper-fabric": "^0.6.0",
    "chai": "^4.4.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
