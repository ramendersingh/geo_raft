{
  "name": "fabric-geo-consensus",
  "version": "1.0.0",
  "description": "Geo-aware etcdraft consensus for Hyperledger Fabric 2.5",
  "main": "index.js",
  "scripts": {
    "test": "mocha tests/**/*.js",
    "benchmark": "node benchmarks/run-benchmark.js",
    "start-network": "./scripts/setup-network.sh",
    "start-monitoring": "node monitoring/dashboard.js",
    "build-chaincode": "./scripts/build-chaincode.sh",
    "deploy-chaincode": "./scripts/deploy-chaincode.sh",
    "caliper-benchmark": "npx caliper launch manager --caliper-workspace ./benchmarks --caliper-networkconfig ./benchmarks/network-config.yaml --caliper-benchconfig ./benchmarks/benchmark-config.yaml",
    "generate-report": "node scripts/generate-report.js",
    "clean": "./scripts/clean.sh"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "chart.js": "^4.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fabric-network": "^2.2.20",
    "helmet": "^7.1.0",
    "html-pdf": "^3.0.1",
    "lodash": "^4.17.21",
    "markdown-pdf": "^11.0.0",
    "node-cron": "^3.0.3",
    "prom-client": "^15.1.0",
    "puppeteer": "^24.12.1",
    "socket.io": "^4.7.2",
    "winston": "^3.10.0",
    "yaml": "^2.3.4"
